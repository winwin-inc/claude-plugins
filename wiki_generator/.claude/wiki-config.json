{
  "output_dir": "docs",
  "exclude_patterns": [
    "node_modules",
    "dist",
    "build",
    ".git",
    "__pycache__",
    "*.pyc",
    ".venv",
    "venv"
  ],
  "template_dir": ".claude/templates",
  "quality_threshold": 80,
  "diagrams": {
    "enabled": true,
    "detail_level": "medium"
  },
  "project_info": {
    "name": "Repo Wiki Generator",
    "version": "1.0.0",
    "language": "zh-CN"
  }
}
